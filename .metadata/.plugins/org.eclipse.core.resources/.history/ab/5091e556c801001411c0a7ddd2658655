package com.hib.entity;

import javax.xml.crypto.Data;


public class Event {
	private Long id;
	private String title;
	private Date data;

	public Long getId() {
		return id;
	}

	public String getTitle() {
		return title;
	}

	public Data getData() {
		return data;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public void setData(Data data) {
		this.data = data;
	}

}
